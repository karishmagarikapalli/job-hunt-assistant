    "search_terms": [
        "Software Developer",
        "Full Stack Developer",
        "Software Engineer",
        "Senior Software Developer",
        "Senior Full Stack Developer",
        "Solution Engineer",
        "Solutions Engineer",
        "Technical Solutions Engineer",
        "Solutions Architect",
        "Developer Advocate",
        "Technical Consultant"
