            "location_selector": ".gc-card__location",
            "link_selector": ".gc-card__link",
            "search_url_template": "https://careers.google.com/jobs/results/?q={search_term}&location={location}"
        },
        {
            "name": "Microsoft",
            "career_url": "https://careers.microsoft.com/us/en/search-results",
